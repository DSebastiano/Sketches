package com.practice.rubrica.repository;

import java.util.List;

import com.practice.rubrica.entities.Contatti;

public interface ContattiRep {

	List<Contatti> selTutti();
	
	List<Contatti> selByInstructions(String instructions);
	
	List<Contatti> selByFavourites(String instructions, Boolean favs);
	
	void inserisci(Contatti contatto);
}
