package com.practice.rubrica.services;

import java.util.List;

import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.stereotype.Service;

import com.practice.rubrica.entities.Registro;
import com.practice.rubrica.repository.RegistroRep;

@Service
public class RegistroServiceImpl implements RegistroService {
	
	@Autowired
	private RegistroRep contattiRep;

	@Override
	public List<Registro> selTutti() {
		
		return contattiRep.selTutti();
	}

	@Override
	public void inserisci(Registro chiamata) {
		
		RegistroRep.inserisci(chiamata);

	}

}
